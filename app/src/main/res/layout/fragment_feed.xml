<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profilebackground"
    tools:context=".UI.Student.FeedFragment">

    <ListView
        android:id="@+id/listEvents"
        android:layout_width="match_parent"
        android:layout_height="565dp"
        android:backgroundTint="#00FFFFFF"
        android:cacheColorHint="#FBFBFB"
        android:divider="#FFFFFF"
        android:dividerHeight="1dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </ListView>

    <ImageButton
        android:id="@+id/filtrosfeed"
        android:layout_width="80dp"
        android:layout_height="76dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="328dp"
        android:layout_marginTop="566dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="89dp"
        android:background="#DA1E1E"
        android:backgroundTint="#00FFFFFF"
        android:src="@drawable/filterbutton"
        tools:ignore="SpeakableTextPresentCheck" />
</RelativeLayout>
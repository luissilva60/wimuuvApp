<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Student.RateActivity">


    <include
        android:id="@+id/include"
        layout="@layout/toolbar2" />

    <EditText
        android:id="@+id/editTextTextMultiLine"
        android:layout_width="322dp"
        android:layout_height="53dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Comentário (Opcional)"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="423dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Avaliação de um evento"
        android:textSize="23sp"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="194dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="156dp"
        android:layout_height="26dp"
        android:text="Avalie o evento de 0-10"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="276dp" />

    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="130dp"
        android:layout_height="49dp"
        android:ems="10"
        android:hint="Rate"
        android:inputType="number"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="317dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#EB1C24"
        android:text="Avaliar"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="542dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comentário:"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="391dp" />


</androidx.constraintlayout.widget.ConstraintLayout>